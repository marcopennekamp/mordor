# Adds a to b.
!test.add (i64 a, i64 b) i64
    load    a
    load    b
    add
    return

# Subtracts b from a.
!test.sub (i64 a, i64 b) i64
    load    a
    load    b
    sub
    return

# Tests the jump ability, yet resulting in a endless loop.
!test.jump void
    i64     a
    i64     b
init:
    load    a
    load    b
math:
    add
end:
    jmp     init